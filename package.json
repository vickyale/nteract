    "remark-parse": "^5.0.0",
    "remark-stringify": "^5.0.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "s3-proxy": "^1.1.0",
    "shell-env": "^2.0.0",
    "spawn-rx": "^2.0.12",
    "spawnteract": "^5.0.0",
    "style-loader": "^0.23.0",
    "styled-jsx": "^2.2.6",
    "tv4": "^1.3.0",
    "unified": "^7.0.0",
    "unist-builder": "^1.0.2",
    "url-join": "^4.0.0",
    "url-search-params": "^1.0.0",
    "util": "^0.10.3",
    "uuid": "^3.1.0",
    "webfontloader": "^1.6.28",
    "webpack": "^4.14.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "winston": "^3.0.0",
    "xhr2": "*",
    "xmlhttprequest": "^1.8.0",
    "yargs": "^12.0.0"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "applications/*",
      "applications/jupyter-extension/nteract_on_jupyter"
    ],
    "nohoist": [
      "**/nteract/**"
    ]
  },
  "resolutions": {
    "node-fetch": "2.1.2"
  }
}
